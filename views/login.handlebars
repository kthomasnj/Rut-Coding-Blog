<h1>LOGIN</h1>

<form class="form-signin">
    <div class="form-group">
        <input class="form-input" placeholder="email" type="text" id="email-login" />
    </div>
    <div class="form-group">
        <input class="form-input" placeholder="password" type="password" id="password-login" />
    </div>
    <div class="form-group">
        <button class="btn btn-primary" type="submit">LOGIN</button>
    </div>
    <a href="/signup">Sign up instead</a>
</form>
</div>

<script src="./js/login.js"></script>